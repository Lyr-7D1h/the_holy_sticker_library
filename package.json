    "fastify-sensible": "^3.1.0",
    "puppeteer": "^5.5.0",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.2",
    "venom-bot": "^2.2.3"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
