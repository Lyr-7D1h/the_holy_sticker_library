{
  "name": "holy_sticker_library",
  "version": "1.0.0",
  "description": "A whatsapp bot for saving and sending stickers",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run start:dev",
    "build": "rm -rf ./build && tsc -p tsconfig.json",
    "start:dev": "NODE_ENV=development tsnd --exit-child --rs --clear ./src",
    "start:prod": "node .",
    "test": "tap test/**/*.test.ts",
    "linter": "npx eslint --ext .ts ./src"
  },
  "keywords": [
    "bot",
    "whatsapp"
  ],
  "author": "Lyr",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.9.2",
    "fastify-autoload": "^3.4.0",
    "fastify-formbody": "^5.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-static": "^3.3.1",
    "pino": "^6.9.0",
    "puppeteer": "^5.5.0",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.2",
    "venom-bot": "^2.2.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/pino": "^6.3.5",
    "@types/sqlite3": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "tap": "^14.11.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
