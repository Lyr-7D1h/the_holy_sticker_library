{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noUnusedLocals": false,
    "jsx": "react-jsx",
    "allowJs": false,
    "module": "esnext",
    "isolatedModules": true,
    "baseUrl": "src",
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../shared"
    }
  ]
}
